@Library('mylibrary')_
pipeline
{
    agent any 
    stages
    {
        stage('Cont-iDownload_stage')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("maven")
                }
            }
        }
        stage('Cont-Build_stage')
        {
            steps
            {
                script
                {
                     cicd.mavenBuild()
		} 
	    }	

         }
     } 

 }

